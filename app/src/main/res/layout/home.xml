<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <ImageView
            android:layout_width="586dp"
            android:layout_height="803dp"
            android:src="@drawable/leaves"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/pozaFrunze"
    />

    <ImageView
            android:layout_width="593dp"
            android:layout_height="161dp"
            android:src="@drawable/transparent_background_bar_navigation"
            app:layout_constraintEnd_toEndOf="@id/pozaFrunze"
            app:layout_constraintStart_toStartOf="@id/pozaFrunze"
            android:background="null"
            android:tint="@color/leavesColor"
            app:layout_constraintHorizontal_bias="0.0" tools:layout_editor_absoluteY="645dp"/>
    <ImageView
            android:layout_width="350dp"
            android:layout_height="231dp"
            android:src="@drawable/GroupTitle"
            app:layout_constraintBottom_toTopOf="@+id/buttonCamera"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textKodama"/>
    <ImageButton
            android:background="null"
            android:id="@+id/buttonCamera"
            android:layout_width="wrap_content"
            android:layout_height="56dp"
            android:src="@drawable/GroupCamera"
            app:layout_constraintTop_toBottomOf="@+id/textKodama"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@id/buttonPicture"/>
    <ImageButton
            android:layout_width="184dp"
            android:layout_height="153dp"
            android:id="@+id/buttonPicture"
            android:src="@drawable/GroupFoto"
            android:background="null"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="@id/buttonPicture"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/HomeButton"
            android:background="null"
            android:id="@+id/homeButton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/historyButton"
            app:layout_constraintTop_toTopOf="@+id/historyButton"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/HistoryButton"
            android:id="@+id/historyButton"
            android:background="null"
            app:layout_constraintStart_toEndOf="@+id/homeButton"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/helpButton" app:layout_constraintTop_toTopOf="@+id/helpButton"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/HelpButton"
            android:id="@+id/helpButton"
            android:background="null"
            app:layout_constraintStart_toEndOf="@+id/historyButton" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/aboutUsButton"
            app:layout_constraintTop_toTopOf="@+id/aboutUsButton"/>

    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/AboutUsButton"
            android:id="@+id/aboutUsButton"
            android:background="null" tools:layout_editor_absoluteY="639dp"
            app:layout_constraintStart_toEndOf="@+id/helpButton" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>



</androidx.constraintlayout.widget.ConstraintLayout>